<script>/*<![CDATA[*/ 
            var sMode_871505 = 'block-behavioral',
                iAttempt = 0,
                iMaxAttempt = 2,
                bDmpTrack = true,
                sImpressionPx = '<img src="https://bcp.crwdcntrl.net/5/c=9613/b=37454954" width="1" height="1" class="hidden" />',
                socialReferrers = ["twitter.com","t.co","facebook.com","fb.me","plus.google.com","goo.gl"],
                referrer = document.referrer.replace(/^(?:https?:\/\/)?(?:www\.)?/i, '').split('/')[0];
            
                // Referrer consideration
                if (referrer && socialReferrers.indexOf(referrer) > -1 ) {
                    sMode_871505 = 'block-contextual';
                    bDmpTrack = false;
                    iMaxAttempt = 1;
                } 
            
            function getRecommendations_871505(aAudiences) {
                iAttempt ++;
                //__tnt.log('\nRunning getRecommendations. Attempt ' +  iAttempt + ' of ' + iMaxAttempt);
                var oEl = document.getElementById('tncms-recommendations-871505' );
                // No audiences passed
                if (!aAudiences || aAudiences.length == 0) {  
                    //__tnt.log('No DMP audiences passed.');
                    
                        if (sMode_871505 != 'block-contextual') {
                            sMode_871505 = 'block-contextual-fallback';
                        }
                    
                    // If contextual set up keyword audiences
                    if (sMode_871505 == 'block-contextual' || sMode_871505 == 'block-contextual-fallback') {
                        bDmpTrack = false; 
                        aAudiences = ["cnn","imported","covid-19","symptom","pandemic","medicine","immunology","pharmacology","anatomy"]
                    }
                    if (!aAudiences || aAudiences.length == 0) {
                        //__tnt.log('No audiences or keywords to work with. Remove block');
                        $("#tncms-block-871505").remove();
                      return;
                    }
                }
                
                for(var nIdx = 0; nIdx < aAudiences.length; nIdx++) {
                    aAudiences[nIdx] = aAudiences[nIdx].replace('keyword:', '');
                }
                //__tnt.log('tncms.behaviors: ' + aAudiences);
                var oRequest = new XMLHttpRequest();
                oRequest.onreadystatechange = function() {
                    if ((oRequest.readyState == 4) && (oRequest.status == 200)) {
                        var oResponse = JSON.parse(oRequest.responseText);
                        if (oResponse.assets.length <= 0) {
                            //__tnt.log('No assets in response.');
                            if (iAttempt < iMaxAttempt) {
                                //__tnt.log('Going in for more assets...');
                                getRecommendations_871505();
                            } else {
                                //__tnt.log('No assets available for current audience.');
                                $("#tncms-block-871505").remove();
                                return false;
                            }
                        }
                        var aAssets = shuffle_871505(oResponse.assets),
                            aAssets = oResponse.assets,
                            sHTML = '',
                            iCount = 0,
                            iMax = 4;
                        for(var nIdx = 0; (nIdx < oResponse.assets.length && iCount < iMax); nIdx++) { 
                            if ( oResponse.assets[nIdx].content.includes('inline-summary') && !(oResponse.assets[nIdx].content.includes('68726b52-4c6a-53e6-94df-fd29ddacbd47'))  ) {
                                iCount++;
                                sHTML += '<div class=" col-sm-3">';
                                /* response content */
                                sHTML += oResponse.assets[nIdx].content;
                                sHTML += '</div>';
                                    if ( iCount % (12 / 3) == 0 ) {
                                        sHTML += '<div class="clearfix hidden-xs"></div>';
                                    }
                            } 
                        }
                        sHTML = sHTML.replace(/#tncms-source/g, '#tncms-source=' + sMode_871505);
                        oEl.innerHTML = sHTML;
                    }
                };
                oRequest.open('GET', '/tncms/search/recommend/?app=editorial&t=article&inline=summary&preview=1&k=' + encodeURIComponent(aAudiences.join(',')));
                oRequest.send();
            }
            // Shuffle assets - Fisher Yates shuffle
            function shuffle_871505(array) {
                var currentIndex = array.length, temporaryValue, randomIndex;
                // While there remain elements to shuffle
                while (0 !== currentIndex) {
                    // Pick a remaining element
                    randomIndex = Math.floor(Math.random() * currentIndex);
                    currentIndex -= 1;
                    // And swap it with the current element
                    temporaryValue = array[currentIndex];
                    array[currentIndex] = array[randomIndex];
                    array[randomIndex] = temporaryValue;
                }
                return array;
            }
            // Click track pixel
            $("#tncms-recommendations-871505.recommendations-container").on("click", "a", function(){
                if (bDmpTrack == true) {
                    $("#tncms-recommendations-871505")
                        .append('<img src="https://bcp.crwdcntrl.net/5/c=9613/b=37454955" width="1" height="1" class="hidden" />');
                }
            });
            if (sMode_871505 == 'block-behavioral') {
                (function (callback) { var oScript; var oStorage; try { oStorage = JSON.parse(sessionStorage.getItem('tncms.behaviors')); if (!oStorage || !oStorage.audiences || !oStorage.expired) { throw "Not in local storage"; } if (oStorage.expired < new Date().getTime()) { sessionStorage.removeItem('tncms.behaviors'); throw "Cache is expired"; } callback(oStorage.audiences); } catch(oException) { if (!window.tncms_aud) { window.tncms_aud = null; } oScript = document.createElement('script'); oScript.onerror = function() { callback([]); }; oScript.onload = function() { try { var oResult = { audiences:[], expired: new Date().getTime() + 86400000 }; if (!window.tncms_aud) { return; } for(var nIdx = 0; nIdx < window.tncms_aud.Profile.Audiences.Audience.length; nIdx++) { var sAbbr = window.tncms_aud.Profile.Audiences.Audience[nIdx].abbr; if (sAbbr.match(/^[a-z0-9]+\:/)) { oResult.audiences[oResult.audiences.length] = sAbbr; } } oResult.audiences.sort(); sessionStorage.setItem('tncms.behaviors', JSON.stringify(oResult)); } catch(oException) { if (console && console.warn) { console.warn('Failed to add behaviors to session storage'); } } callback(oResult.audiences); }; oScript.setAttribute('async', true); oScript.setAttribute('src', '//ad.crwdcntrl.net/5/c=9613/pe=y/var=tncms_aud'); var aHead = document.getElementsByTagName("head"); if (aHead && aHead[0]) { if (TNCMS && TNCMS.Tracking) { var oTracker = TNCMS.Tracking.createBucket({ 'app': 'tncms-dmp', 'metric': 'audience-extraction', 'data': { name: 'client', value: 9613 } }); oTracker.addEvent(new Date().getTime()); } aHead[0].appendChild(oScript); } } })(getRecommendations_871505);} else {
                getRecommendations_871505();
            }// Impression track pixel
            function trackImpression() {
                if (bDmpTrack == true) {
                    $("[data-lotame-track]").withinviewport().removeAttr("data-lotame-track").append(sImpressionPx);
                }
            }
            $(document).ready(trackImpression).on("scrollstop", trackImpression); /*]]>*/</script>
