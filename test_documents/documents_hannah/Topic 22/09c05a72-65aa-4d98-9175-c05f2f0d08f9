ag {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/sale.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .custom-classifieds-icons .classifieds-categories .fa.fa-paw {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/pets.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .custom-classifieds-icons .classifieds-categories .panel:nth-child(7) .fa.fa-tag {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/legal.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .custom-classifieds-icons .classifieds-categories .fa.fa-wrench {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/services.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .custom-classifieds-icons .classifieds-categories .panel:nth-child(9) .fa.fa-wrench {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/agreement.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .custom-classifieds-icons .classifieds-categories .fa.fa-users {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/announcements.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .custom-classifieds-icons .classifieds-categories .panel:nth-child(10) .fa.fa-users {background:#fff url("libraries/flex/components/site/resources/images/classified-icons/judge.png") no-repeat top left scroll;height: 32px;width: 47px;text-indent: -9999px;} .app-classifieds .card figure .image .asset-icon {display: none;} @media (min-width: 992px) and (max-width: 1200px) { .col-lg-8 {width:66%} .col-lg-4 {width:34%;} .col-lg-pull-8 {right: 66%;} .col-lg-push-8{left:66%;} .col-lg-pull-4 {right: 34%;} .col-lg-push-4{left:34% } .col-lg-offset-8 {margin-left: 66%} .col-lg-offset-4{ margin-le