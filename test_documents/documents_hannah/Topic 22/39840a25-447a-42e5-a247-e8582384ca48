$(document).ready(function() {
    $('.region-switch').click(function(e){
		e.stopPropagation();
		if ( $(this).hasClass('region-switch-opn') ){
			$(this).removeClass('region-switch-opn');
			$('.region-dorpdown').slideUp();
		}
		else{
			$(this).addClass('region-switch-opn');
			$('.region-dorpdown').slideDown();
		}
	});	
	$('.region-dorpdown').click(function(e){
		e.stopPropagation();
	});
	$('.region-dorpdown .choose-locatoion').click(function(){
		$('.choose-locatoion').removeClass('blue');
		$(this).addClass('blue');		
	});
	$('html').click(function(){
		$('.region-switch').removeClass('region-switch-opn');
		$('.region-dorpdown').slideUp();
	});
 	var mhUser = sessionStorage.getItem("mhUser");
 	if(mhUser=='true'){
 	document.getElementById('d2').style.display='none';
	document.getElementById('d1').style.display='block';
 	$("a.region-switch").text("Maharashtra");
 	$("#mh").addClass("blue");
 	}else{
 	document.getElementById('d1').style.display='none';
	document.getElementById('d2').style.display='block';
 	$("a.region-switch").text("Outside Maharashtra");
 	$("#omh").addClass("blue");
 	}
 	
	$(".confirm-btn").click(function(){
	var id = $(".choose-locatoion.blue").attr("id");
	if(id=='mh'){
	sessionStorage.setItem("mhUser", true);
	document.location.href = '/mumbai-news';
	}else{
	sessionStorage.setItem("mhUser", false);
	document.location.href = '/';
	}
  });
	/*Hambergr Menu*/
	$('.hamMenubtn').click(function(ee){
		ee.stopPropagation();
		if ( $('.hamburgerMenu').hasClass('opn') ){
			$('.hamburgerMenu').removeClass('opn');
			$('.hamburgerMenuDropDown').slideUp();
			$('.overley').fadeOut();
		}
		else{
			$('.hamburgerMenu').addClass('opn');
			$('.hamburgerMenuDropDown').slideDown();
			$('.overley').fadeIn();
			//$('body').css('overflow','hidden');
		}		
	});		
	$(document).click(function(){
		$('.hamburgerMenu').removeClass('opn');
		$('.hamburgerMenuDropDown').slideUp();
		$('.overley').fadeOut();
		//$('body').css('overflow','auto');
	});
	$('.hamburgerMenuDropDown').click(function(e){
		e.stopPropagation();
	});
	/*Hambergr Menu End*/
});
	
</script>
